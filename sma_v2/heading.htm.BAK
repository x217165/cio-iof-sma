<SCRIPT language="javascript" type="text/javascript">
<!--hide the script from old browsers
function fct_selApp() {
//***************************************************************************************************
// Function:	fct_selApp															                *
// Purpose:		To open a new browser window with the selected application							*
//																									*
// Created By:	Gilles Archer 09/19/2000															*
//																									*																				*
//***************************************************************************************************
	var strAppName = document.frmPageTitle.selApps.value;

	switch (strAppName){
		case 'SRTII':
			document.frmPageTitle.selApps.selectedIndex = 0;
			//window.open('http://tacjagr3/srt2');
			window.open('http://abvdev023/srt2');
			break;
		case 'ESDReports':
			document.frmPageTitle.selApps.selectedIndex = 0;
			window.open('http://tacjagr2/esdreports');
			//window.open ('http://tacabcbrs01/esdreports');
			break;
		case 'SMA2':
			//do nothing
	}
}
//--> End hide script
</SCRIPT>
